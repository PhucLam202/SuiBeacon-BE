{
  "name": "suibeacon",
  "version": "1.0.0",
  "description": "A basic CLI tool for downloading files",
  "main": "dist/cli.js",
  "bin": {
    "beacon": "./dist/cli.js"
  },
  "type": "module",
  "scripts": {
    "build": "npm run ensure-nix && tsc --project tsconfig.json",
    "start": "node dist/cli.js",
    "dev": "nodemon --ext ts,js,json --exec 'tsx src/app.ts'",
    "link": "npm link --global",
    "ensure-nix": "node scripts/ensure-nix.js"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@mysten/sui": "^1.28.0",
    "@mysten/wallet-standard": "^0.14.5",
    "@mysten/walrus": "^0.0.17",
    "boxen": "^8.0.1",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "http-status-codes": "^2.3.0",
    "mongoose": "^8.14.0",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.1.9",
    "ora": "^8.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^22.10.2",
    "@types/node-fetch": "^2.6.12",
    "pnpm": "^8.0.0",
    "tsx": "^4.19.3",
    "typescript": "5.8.3"
  }
}
